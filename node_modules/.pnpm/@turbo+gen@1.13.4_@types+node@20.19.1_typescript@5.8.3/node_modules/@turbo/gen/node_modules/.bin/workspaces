#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/miro/projects/PTLS/react/NEU/revierkompass22/node_modules/.pnpm/@turbo+workspaces@1.13.4/node_modules/@turbo/workspaces/dist/node_modules:/home/miro/projects/PTLS/react/NEU/revierkompass22/node_modules/.pnpm/@turbo+workspaces@1.13.4/node_modules/@turbo/workspaces/node_modules:/home/miro/projects/PTLS/react/NEU/revierkompass22/node_modules/.pnpm/@turbo+workspaces@1.13.4/node_modules/@turbo/node_modules:/home/miro/projects/PTLS/react/NEU/revierkompass22/node_modules/.pnpm/@turbo+workspaces@1.13.4/node_modules:/home/miro/projects/PTLS/react/NEU/revierkompass22/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/miro/projects/PTLS/react/NEU/revierkompass22/node_modules/.pnpm/@turbo+workspaces@1.13.4/node_modules/@turbo/workspaces/dist/node_modules:/home/miro/projects/PTLS/react/NEU/revierkompass22/node_modules/.pnpm/@turbo+workspaces@1.13.4/node_modules/@turbo/workspaces/node_modules:/home/miro/projects/PTLS/react/NEU/revierkompass22/node_modules/.pnpm/@turbo+workspaces@1.13.4/node_modules/@turbo/node_modules:/home/miro/projects/PTLS/react/NEU/revierkompass22/node_modules/.pnpm/@turbo+workspaces@1.13.4/node_modules:/home/miro/projects/PTLS/react/NEU/revierkompass22/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@turbo+workspaces@1.13.4/node_modules/@turbo/workspaces/dist/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../@turbo+workspaces@1.13.4/node_modules/@turbo/workspaces/dist/cli.js" "$@"
fi
