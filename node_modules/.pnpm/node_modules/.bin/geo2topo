#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/miro/projects/PTLS/react/NEU/revierkompass22/node_modules/.pnpm/topojson-server@3.0.1/node_modules/topojson-server/bin/node_modules:/home/miro/projects/PTLS/react/NEU/revierkompass22/node_modules/.pnpm/topojson-server@3.0.1/node_modules/topojson-server/node_modules:/home/miro/projects/PTLS/react/NEU/revierkompass22/node_modules/.pnpm/topojson-server@3.0.1/node_modules:/home/miro/projects/PTLS/react/NEU/revierkompass22/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/miro/projects/PTLS/react/NEU/revierkompass22/node_modules/.pnpm/topojson-server@3.0.1/node_modules/topojson-server/bin/node_modules:/home/miro/projects/PTLS/react/NEU/revierkompass22/node_modules/.pnpm/topojson-server@3.0.1/node_modules/topojson-server/node_modules:/home/miro/projects/PTLS/react/NEU/revierkompass22/node_modules/.pnpm/topojson-server@3.0.1/node_modules:/home/miro/projects/PTLS/react/NEU/revierkompass22/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../topojson-server/bin/geo2topo" "$@"
else
  exec node  "$basedir/../topojson-server/bin/geo2topo" "$@"
fi
